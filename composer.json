{
    "name": "form-relay/request",
    "type": "library",
    "description": "FormRelay route and dispatcher for (http) requests.",
    "keywords": [
        "framework",
        "form",
        "data",
        "submission",
        "relay",
        "request"
    ],
    "homepage": "https://www.mediatis.de",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Michael VÃ¶hringer",
            "role": "Developer"
        },
        {
            "name": "Meelis Karulin",
            "role": "Developer"
        },
        {
            "name": "Damian Ickler",
            "role": "Developer"
        },
        {
            "name": "Stephan Ude",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "guzzlehttp/guzzle": "^6.0||^7.0",
        "form-relay/core": "dev-feature/LMSM22-105-route-context-updates"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 || ^8.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "FormRelay\\Request\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FormRelay\\Request\\Tests\\": "tests/",
            "FormRelay\\Core\\Tests\\": "vendor/form-relay/core/tests/"
        }
    },
    "scripts": {
      "test": "./vendor/bin/phpunit --bootstrap vendor/autoload.php tests/",
      "testdox": "./vendor/bin/phpunit --bootstrap vendor/autoload.php --testdox --colors=always tests/"
    }
}
